	gmmem_maxiters="100"
	gmmem_min_obsprior="0.2"
	gmmem_nrestarts_firstframe="10"
	gmmem_precision="0.01"
	ignorebowls=""
	lastframetrack="Inf"
	minccarea="20"
	typebigval="F"
	typefield="sex"
	typesmallval="M"
	usemediandt="1"/>

<wingtrack
	dosetwingtrack="0"
	max_wingcc_dist="2.35619"
	max_wingpx_angle="2.35619"
	min_nonzero_wing_angle="0.174533"
	min_single_wing_area="10"
